INFLUXDB_DATA_ENGINE=tsm1
INFLUXDB_REPORTING_DISABLED=false
INFLUXDB_HTTP_AUTH_ENABLED=false

# Docker initialization settings
DOCKER_INFLUXDB_INIT_MODE=setup
DOCKER_INFLUXDB_INIT_USERNAME=admin
DOCKER_INFLUXDB_INIT_PASSWORD=your_password_here
DOCKER_INFLUXDB_INIT_ORG=your_org_name
DOCKER_INFLUXDB_INIT_BUCKET=your_bucket_name
DOCKER_INFLUXDB_INIT_RETENTION=0
DOCKER_INFLUXDB_INIT_ADMIN_TOKEN=your_admin_token_here

# HTTP API Settings
INFLUXDB_HTTP_BIND_ADDRESS=:8086
INFLUXDB_HTTP_IDLE_TIMEOUT=5m

# Storage Settings
INFLUXDB_STORAGE_WAL_FSYNC_DELAY=0s
INFLUXDB_STORAGE_CACHE_MAX_MEMORY_SIZE=1073741824

# Logging Settings
INFLUXDB_LOGGING_LEVEL=info
INFLUXDB_LOGGING_SUPPRESS_LOGO=false

